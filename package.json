{
  "name": "modale_npm_lib",
  "version": "1.0.4",
  "description": "A simple react component library for the hrnet project (OpenClassrooms)",
  "main": "dist/index",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.es.js"
    },
    "./styles.css": {
      "require": "./dist/styles.css",
      "default": "./dist/styles.css"
    }
  },
  "files": [
    "dist/**/*",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist",
    "build": "rollup -c",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MargauxLeroy/modale_npm_lib.git"
  },
  "keywords": [
    "React",
    "OpenClassrooms",
    "Modal"
  ],
  "author": "Margaux Leroy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MargauxLeroy/modale_npm_lib/issues"
  },
  "homepage": "https://github.com/MargauxLeroy/modale_npm_lib#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.1",
    "@storybook/addon-essentials": "^7.0.8",
    "@storybook/addon-interactions": "^7.0.8",
    "@storybook/addon-links": "^7.0.8",
    "@storybook/blocks": "^7.0.8",
    "@storybook/react": "^7.0.8",
    "@storybook/react-webpack5": "^7.0.8",
    "@storybook/testing-library": "^0.0.14-next.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.79.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "storybook": "^7.0.8"
  },
